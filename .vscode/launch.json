{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Install prereqs",
            "type": "debugpy",
            "request": "launch",
            "module": "pip",
            "args": [
                "install",
                "pillow",
                "Kivy"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Gen solution",
            "type": "debugpy",
            "request": "launch",
            "program": "tests/pickleImages.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Distribute files",
            "type": "debugpy",
            "request": "launch",
            "program": "admin/updateStudentsFiles.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "PythoShop",
            "type": "debugpy",
            "request": "launch",
            "program": "PythoShop.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Tests",
            "type": "debugpy",
            "request": "launch",
            "program": "tests/testRunner.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Test All",
            "type": "debugpy",
            "request": "launch",
            "module": "gradeAll",
            "args": [
                ">",
                "testResults.tsv",
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/tests",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/admin:${workspaceFolder}/tests"
            }
        },
        {
            "name": "Test One",
            "type": "debugpy",
            "request": "launch",
            "module": "gradeOne",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/tests",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:${workspaceFolder}/admin:${workspaceFolder}/tests"
            }
        },
        {
            "name": "Plagiarism check",
            "type": "debugpy",
            "request": "launch",
            "module": "plagiarismCheck",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/admin",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}